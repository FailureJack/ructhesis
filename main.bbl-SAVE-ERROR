      \strng{namehash}{b6a332656cbbed472eff9ced0042dc38}
      \strng{fullhash}{b6a332656cbbed472eff9ced0042dc38}
      \strng{bibnamehash}{b6a332656cbbed472eff9ced0042dc38}
      \strng{authorbibnamehash}{b6a332656cbbed472eff9ced0042dc38}
      \strng{authornamehash}{b6a332656cbbed472eff9ced0042dc38}
      \strng{authorfullhash}{b6a332656cbbed472eff9ced0042dc38}
